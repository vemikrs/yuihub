{
  "name": "@yuihub/server",
  "version": "1.0.0-beta.23",
  "description": "YuiHub Server - Semantic Memory API for Agentic AI",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vemikrs/yuihub.git",
    "directory": "apps/v1/backend"
  },
  "keywords": [
    "yuihub",
    "semantic-memory",
    "ai",
    "agent",
    "api-server"
  ],
  "bin": {
    "yuihub-server": "./dist/server.js",
    "yuihub": "./dist/server.js"
  },
  "exports": {
    ".": "./dist/server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsc -b",
    "dev": "npx tsx watch src/server.ts",
    "start": "node dist/server.js",
    "setup": "npx tsx src/cli/setup.ts",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "vitest run",
    "test:e2e": "vitest run tests/e2e",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fastify/bearer-auth": "^10.1.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/rate-limit": "^10.0.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.2.5",
    "@google-cloud/vertexai": "^1.10.0",
    "@lancedb/lancedb": "^0.23.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.23",
    "@xenova/transformers": "^2.17.2",
    "@yuihub/core": "workspace:*",
    "async-mutex": "^0.5.0",
    "async-rwlock": "^1.1.1",
    "chokidar": "^5.0.0",
    "cron": "^4.4.0",
    "fastify": "^5.7.2",
    "fastify-type-provider-zod": "^6.1.0",
    "fastq": "^1.20.1",
    "fs-extra": "^11.3.3",
    "inquirer": "^13.2.2",
    "lodash": "^4.17.23",
    "simple-git": "^3.30.0",
    "tiny-segmenter": "^0.2.0",
    "tree-sitter": "^0.25.0",
    "tree-sitter-javascript": "^0.25.0",
    "tree-sitter-typescript": "^0.23.2",
    "ulid": "^3.0.2",
    "web-tree-sitter": "^0.26.3",
    "zod": "^4.3.6",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^25.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
