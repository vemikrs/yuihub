{
  "name": "yuihub-workspace",
  "version": "0.2.1",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "test": "pnpm run test:v1",
    "test:v1": "pnpm --filter @yuihub/v1-backend test",
    "build:core": "pnpm --filter @yuihub/core build",
    "build:backend": "pnpm --filter @yuihub/v1-backend build",
    "build:client": "pnpm --filter yuihub-copilot-chat build",
    "build:all-v1": "pnpm build:core && pnpm build:backend && pnpm build:client",
    "build-all": "pnpm build:all-v1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "concurrently": "^9.2.1"
  },
  "dependencies": {}
}
